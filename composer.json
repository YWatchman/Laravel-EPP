{
  "name": "yvanwatchman/laravel-epp",
  "minimum-stability": "dev",
  "version": "1.0.0",
  "description": "Laravel package to work with EPP (SIDN)",
  "type": "library",
  "prefer-stable": true,
  "require": {
    "php": "^7.2",
    "laravel/framework": "5.8.32",
    "metaregistrar/php-epp-client": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "YWatchman\\LaravelEPP\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YWatchman\\LaravelEPP\\ServiceProvider"
      ]
    },
    "aliases": {
      "Epp": "YWatchman\\LaravelEPP\\Epp"
    }
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Yvan E. Watchman",
      "email": "ywatchman@cyberfusion.nl"
    }
  ]
}
